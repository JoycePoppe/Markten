<% include header%>
<div class="midden">
  <div class="detail">
    <ul>
      <li class="vet"> Locatie </li>
      <li><%=markt.attributes.locatie %></li>
      <li class="vet"> District </li>
      <li><%=markt.attributes.district %></li>
      <li class="vet"> Soort markt </li>
      <li><%=markt.attributes.soort %></li>
      <li class="vet"> Marktdag(en) </li>
      <li><%=markt.attributes.marktdag1 %></li>
      <% if (markt.attributes.marktdag2 != "geen") {%>
      <li><%=markt.attributes.marktdag2 %></li>
      <% } %>
      <li class="vet"> Markt van - tot </li>
      <li><%=markt.attributes.van %> - <%=markt.attributes.tot %></li>
    </ul>
  </div>
</div>
<% include footer%>
